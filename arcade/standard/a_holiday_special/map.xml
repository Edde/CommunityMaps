<map proto="1.5.0">
<name>A Holiday Special</name>
<objective>Race through the OCN 2014 holiday lobby and reach the end of the maze first!</objective>
<version>1.0.0</version>
<phase>staging</phase>
<authors>
    <author uuid="19388047-f04b-43ac-9546-22aa69aea75c" contribution="Create the lobby"/> <!-- Bleanny -->
    <author uuid="a9f11454-beac-4cc3-a138-1780802c8794" contribution="XML"/> <!-- Crazy_ -->
</authors>
<created>2024-11-13</created>
<game>Race</game>
<players max="25" colors="true"/>
<kits>
    <kit id="night-vision">
        <effect>night vision</effect>
    </kit>
    <lend kit="night-vision">
        <filter>
            <score>1</score>
        </filter>
    </lend>
</kits>
<spawns>
    <spawn region="platform" yaw="-45"/>
    <default region="platform" yaw="-45"/>
</spawns>
<regions>
    <cylinder id="platform" base="141.5,14,-40.5" radius="2.5" height="1"/>
    <cuboid id="checkpoint" min="183,23,1" max="182,25,0"/>
    <cuboid id="finish" min="151,4,-31" max="149,6,-33"/>
    <apply block="never"/>
</regions>
<filters>
    <all id="at-0">
        <not>
            <any>
                <score>1</score>
                <score>2</score>
            </any>
        </not>
    </all>
</filters>
<score>
    <limit>2</limit>
    <box region="checkpoint" points="1" filter="at-0"/>
    <box region="finish" points="1"/>
</score>
<disabledamage>
    <damage>entity attack</damage>
    <damage>fall</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
</map>
