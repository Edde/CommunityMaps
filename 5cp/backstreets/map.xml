<map proto="1.5.0">
<name>Backstreets</name>
<version>1.1.6</version>
<variant id="5v5">5v5</variant>
<objective>Capture all five control points.</objective>
<include id="5cp"/>
<constant id="team-1-name">Blue</constant>
<constant id="team-2-name">Red</constant>
<constant id="damage-resistance">3s</constant>
<unless variant="5v5">
    <constant id="respawn-timer">4s</constant>
    <include id="conquest"/>
</unless>
<if variant="5v5">
    <constant id="respawn-timer">6s</constant>
    <include id="conquest-comp"/>
</if>
<authors>
    <author uuid="2ca8072f-74be-4798-85b8-bbce03aa91af"/> <!-- Tywnis -->
</authors>
<teams>
    <team id="team-1" color="blue" max="5" max-overfill="5">${team-1-name}</team>
    <team id="team-2" color="dark red" max="5" max-overfill="5">${team-2-name}</team>
</teams>
<spawns>
    <default>
        <region yaw="90">
            <point>-187.5,80,18.5</point>
        </region>
    </default>
    <spawn team="team-1" kit="base-kit">
        <region yaw="90">
            <point>-145.5,39,-31.5</point>
        </region>
    </spawn>
    <spawn team="team-2" kit="base-kit">
        <region yaw="-90">
            <point>-229.5,39,-31.5</point>
        </region>
    </spawn>
</spawns>
<regions>
    <cuboid id="cp-team-1-last" min="-147,45,-29" max="-152,49,-34"/>
    <cuboid id="cp-team-1-2nd" min="-190,32,-67" max="-185,36,-62"/>
    <cuboid id="cp-mid" min="-190,36,-34" max="-185,40,-29"/>
    <cuboid id="cp-team-2-2nd" min="-190,32,-1" max="-185,36,4"/>
    <cuboid id="cp-team-2-last" min="-228,45,-29" max="-223,49,-34"/>
    <apply block="never" use="never"/>
</regions>
<spawners>
    <spawner max-entities="2" min-delay="3s" max-delay="10s">
        <spawn-region>
            <cuboid min="-188,29,-32" max="-187,30,-31"/>
        </spawn-region>
        <player-region>
            <cuboid min="-189,29,-33" max="-186,30,-30"/>
        </player-region>
        <item material="golden apple"/>
    </spawner>
</spawners>
</map>